{
  "rewrites": [{
    "source": "/:path*",
    "destination": "/html",
    "has": [
      {
        "type": "header",
        "key": "Accept",
        "value": "text/html"
      }
    ]
  }, {
    "source": "/r/:subreddit/comments/:path*",
    "destination": "/comments/:path*?subreddit=:subreddit"
  }, {
    "source": "/u/:path*",
    "destination": "/user/:path*"
  }, {
    "source": "/:path*.json",
    "destination": "/:path*"
  }, {
    "source": "/:path*/.json",
    "destination": "/:path*"
  }, {
    "source": "/:path*",
    "destination": "/api/:path*"
  }]
}
