{
  "rewrites": [{
    "source": "/:path*",
    "destination": "/html",
    "has": [
      {
        "type": "header",
        "key": "Accept",
        "value": "text/html"
      }
    ]
  }, {
    "source": "/api/subreddit_autocomplete:v2",
    "destination": "/api/autocomplete?v2=:v2"
  }, {
    "source": "/r/:group/:path*",
    "destination": "/:path*?group=:group"
  }, {
    "source": "/comments/:id/:slug",
    "destination": "/comments/:id"
  }, {
    "source": "/u/:path*",
    "destination": "/user/:path*"
  }, {
    "source": "/:path*.json",
    "destination": "/:path*"
  }, {
    "source": "/:path*/.json",
    "destination": "/:path*"
  }, {
    "source": "/:path*",
    "destination": "/api/:path*"
  }]
}
